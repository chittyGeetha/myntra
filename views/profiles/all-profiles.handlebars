<section>
    <article class="container my-4 custom_block">
        <table class="table table-bordered">
            <thead class="thead-dark">
                <tr>
                    <th>Photo</th>
                    <th>Firstname</th>
                    <th>Lastname</th>
                    <th>phone</th>
                    <th>Address</th>
                    <th>alternate Address</th>
                    <th>Gender</th>
                    <th>Country</th>
                    <th>Pincode</th>
                    <th>Landmark</th>
                </tr>
            </thead>
            {{#each profile}}
            <tbody>
                <tr>
                    {{#if this.photo}}
                    <td><img src="{{removeFirst6Char photo.0.path}}"/></td>
                    {{else}}
                    <td><img src="" alt="" /></td>
                    {{/if}}
         
                <td>{{firstname}}</td>
                <td>{{lastname}}</td>
                <td>{{phone}}</td>
                <td>{{address}}</td>
                <td>{{alt_address}}</td>
                <td>{{gender}}</td>
                <td>{{country}}</td>
                <td>{{pincode}}</td>
                <td>{{landmark}}</td>
                </tr>   
                
                

            </tbody>
            {{else}}
                {{/each}}
        </table>
    </article>
</section>